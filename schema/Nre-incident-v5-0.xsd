<?xml version="1.0" encoding="UTF-8"?><xsd:schema xmlns="http://nationalrail.co.uk/xml/incident" xmlns:xsd="http://www.w3.org/2001/XMLSchema" xmlns:gms="http://www.govtalk.gov.uk/CM/gms-xs" xmlns:com="http://nationalrail.co.uk/xml/common" targetNamespace="http://nationalrail.co.uk/xml/incident" elementFormDefault="qualified" attributeFormDefault="unqualified" version="4.0" id="nre-incident">  <xsd:annotation>    <xsd:documentation>The National Rail Enquiries Incident Schema</xsd:documentation>    <xsd:appinfo>      <Metadata xmlns="http://www.govtalk.gov.uk/metadata/egms">        <Contributor Role="developer" Href="mailto:geoff.parkin@cds.co.uk">Geoff Parkin</Contributor>        <Contributor Role="developer">Neil Fursey</Contributor>        <Contributor Role="client" Href="mailto:nre@nationalrail.co.uk">NRE</Contributor>        <Creator>NRE</Creator>        <Created>2013-07-01</Created>        <Modified>2014-03-17</Modified>        <Description>The National Rail Enquiries (NRE) Incident XML Schema provides definition for the exchange of structured incidents.</Description>        <Format Scheme="http://www.iana.org/assignments/media-types/">text/xml</Format>        <Identifier Href="http://kb.nationalrail.co.uk/schemas/NreIncidentsSchema">NreIncidents Schema</Identifier>        <Language codeListID="ISO639-2">eng</Language>        <Publisher Href="mailto:nre@nationalrail.co.uk"> National Rail Enquiries, 40 Bernard Street, London WC1N 1BY</Publisher>        <ConformsTo>http://www.w3.org/2001/XMLSchema</ConformsTo>        <HasPart>nre-common-v5-0.xsd</HasPart>        <ProvidesDefinitionOf Namespace="http://nationalrail.co.uk/xml/incident">PtIncidentStructure</ProvidesDefinitionOf>        <ProvidesDefinitionOf Namespace="http://nationalrail.co.uk/xml/incident">SourceStructure</ProvidesDefinitionOf>        <ProvidesDefinitionOf Namespace="http://nationalrail.co.uk/xml/incident">AffectsStructure</ProvidesDefinitionOf>        <ProvidesDefinitionOf Namespace="http://nationalrail.co.uk/xml/incident">InfoLinkStructure</ProvidesDefinitionOf>        <ProvidesDefinitionOf Namespace="http://nationalrail.co.uk/xml/incident">OperatorsStructure</ProvidesDefinitionOf>        <ProvidesDefinitionOf Namespace="http://nationalrail.co.uk/xml/incident">NetworksStructure</ProvidesDefinitionOf>        <ProvidesDefinitionOf Namespace="http://nationalrail.co.uk/xml/incident">AffectedOperatorStructure</ProvidesDefinitionOf>        <ProvidesDefinitionOf Namespace="http://nationalrail.co.uk/xml/incident">AffectedNetworkStructure</ProvidesDefinitionOf>        <ProvidesDefinitionOf Namespace="http://nationalrail.co.uk/xml/incident">IncidentIdentifierType</ProvidesDefinitionOf>        <Copyright href="http://www.opsi.gov.uk/advice/crown-copyright/index.htm">National Rail Enquiries, 2003, 2004</Copyright>        <ProtectiveMarking>unclassified</ProtectiveMarking>        <Status>Draft</Status>        <StatusDescription>Initial release for consultation</StatusDescription>        <Category codeListID="">          Arts, recreation and travel, Tourism, Travel (tourism), Transport, Public transport, Business and industry,          Air transport, Airports, Accidents (air travel),  Air crashes,  Aircraft (civilian),  Aviation,  Safety (air transport),          Low flying aircraft (civilian),  Crashes (aircraft),          Ports and maritime transport, Ferries (marine),          Water transport (sea-going),   Sea transport,   Shipping,   Maritime transport,          Bus services, Coach services, Bus stops and stations,          Rail transport, Railway stations and track, Train services, Underground trains, Accidents (railways),   Bridges (railway),          Rail crashes,   Safety (rail transport),  Infrastructure (railways),  Crashes (trains),          Roads and road transport, Bridges (road),          Vehicles (road use),   Street lighting,  Street repairs,  Taxis,  Traffic congestion ,          Motorways,  Parking,  Pavements,  Infrastructure (roads),  Lorries,  Minicabs,  Heavy goods vehicles,  Highways,  Road signs,          Road works,  Car parks,  Cars (road vehicles),  Congestion (road traffic),  Cycling (transport) , Walking (as transport)        </Category>        <Version>5.0</Version>        <Project>National Rail Enquiries Knowledgebase</Project>        <Title>NRE Incidents XML Schema,  a schema to provide definiiotn for teh exchange of structured incident data</Title>        <Type>message</Type>      </Metadata>    </xsd:appinfo>  </xsd:annotation>  <!-- ======================================================================= -->  <xsd:import namespace="http://nationalrail.co.uk/xml/common" schemaLocation="nre-common-v5-0.xsd">    <xsd:annotation>      <xsd:documentation>        A com set of types for NRE schemas.      </xsd:documentation>    </xsd:annotation>  </xsd:import>  <!-- ======================================================================= -->  <!-- Root element -->  <xsd:element name="Incidents">    <xsd:annotation>      <xsd:documentation>Public Transport Incidents.</xsd:documentation>    </xsd:annotation>    <xsd:complexType>      <xsd:sequence>        <xsd:element ref="PtIncident" minOccurs="0" maxOccurs="unbounded"/>      </xsd:sequence>    </xsd:complexType>  </xsd:element>  <xsd:element name="PtIncident" type="PtIncidentStructure">    <xsd:annotation>      <xsd:documentation>Details of a single incident</xsd:documentation>    </xsd:annotation>  </xsd:element>  <!-- ======================================================================= -->  <!-- Complex types -->  <xsd:complexType name="PtIncidentStructure">    <xsd:annotation>      <xsd:documentation>Type for individual Incident.</xsd:documentation>    </xsd:annotation>    <xsd:sequence>      <xsd:element name="CreationTime" type="xsd:dateTime">        <xsd:annotation>          <xsd:documentation>Time of creation of incident</xsd:documentation>        </xsd:annotation>      </xsd:element>      <xsd:element name="ChangeHistory" type="com:ChangeHistoryStructure">        <xsd:annotation>          <xsd:documentation>Who changed the data most recently.</xsd:documentation>        </xsd:annotation>      </xsd:element>      <xsd:element name="ParticipantRef" type="xsd:NMTOKEN" minOccurs="0">        <xsd:annotation>          <xsd:documentation>Unique identifier of system issuing entry identifier. If absent, taken from Context element. May be different from that in Context, indicating that the incident is forwarded from another system  - without being allocated a new identifier by the inbtermediate system. Note that the ExternalCode may be used to retain the external System's identifier to allow round trip processing. </xsd:documentation>        </xsd:annotation>      </xsd:element>      <xsd:element name="IncidentNumber" type="IncidentIdentifierType">        <xsd:annotation>          <xsd:documentation>Identifier of entry. Must be unique within Participant's current data horizon. Monotonically increasing,  seqience with time of issue. Normally also unique within Participant (ie also outside of the current horizon) so that a uniform namespace can also be used for archived messages as well.  </xsd:documentation>        </xsd:annotation>      </xsd:element>      <xsd:element name="Version" type="xsd:integer" minOccurs="0">        <xsd:annotation>          <xsd:documentation>Version number if entry is update to a previous version. Unique within IncidentNumber. Monotonically increasing within IncidentNumber.  Any values for  classification, description, affects, effects that are present in an update replace any values on previous incidents and updates with the same identifier.  Values that are not updated remain in effect. </xsd:documentation>        </xsd:annotation>      </xsd:element>      <xsd:element name="Source" type="SourceStructure" minOccurs="0">        <xsd:annotation>          <xsd:documentation>Information about source of information, that is, where the agent using the capture client obtained an item of information, or in the case of an automated feed, an identifier of the specific feed.  Can be used to obtain updates, verify details or otherwise assess relevance.</xsd:documentation>        </xsd:annotation>      </xsd:element>      <xsd:element name="OuterValidityPeriod" type="com:HalfOpenTimestampRangeStructure" minOccurs="0">        <xsd:annotation>          <xsd:documentation/>        </xsd:annotation>      </xsd:element>      <!-- OuterValidityPeriod -->      <xsd:element name="ValidityPeriod" maxOccurs="unbounded">        <xsd:annotation>          <xsd:documentation>Overall inclusive Period of applicability of incident</xsd:documentation>        </xsd:annotation>        <xsd:complexType>          <xsd:complexContent>            <xsd:extension base="com:HalfOpenTimestampRangeStructure"/>          </xsd:complexContent>        </xsd:complexType>      </xsd:element>      <xsd:element name="Planned" type="xsd:boolean" default="false">        <xsd:annotation>          <xsd:documentation>Whether the incident was planned (eg engineering works) or unplanned (eg service alteration). Default is false, i.e. unplanned.</xsd:documentation>        </xsd:annotation>      </xsd:element>      <xsd:element name="Summary" type="com:PopulatedStringType">        <xsd:annotation>          <xsd:documentation>Summary of incident. If absent should be generated from structure elements / and or by condensing Description. </xsd:documentation>        </xsd:annotation>      </xsd:element>      <xsd:element name="Description" type="com:PopulatedStringType">        <xsd:annotation>          <xsd:documentation>Description of incident. Should not repeat any strap line incldued in Summary.</xsd:documentation>        </xsd:annotation>      </xsd:element>      <xsd:element name="InfoLinks" minOccurs="0">        <xsd:annotation>          <xsd:documentation>Hyperlinks to other resources associated with incident.</xsd:documentation>        </xsd:annotation>        <xsd:complexType>          <xsd:sequence>            <xsd:element name="InfoLink" type="InfoLinkStructure" maxOccurs="unbounded">              <xsd:annotation>                <xsd:documentation>Hyperlink description</xsd:documentation>              </xsd:annotation>            </xsd:element>          </xsd:sequence>        </xsd:complexType>      </xsd:element>      <xsd:element name="Affects" type="AffectsStructure" minOccurs="0">        <xsd:annotation>          <xsd:documentation>Structured model identifiying parts of transport network affected by incident. Operator and Network values will be defaulted to values in general Context unless explicitly overridden.</xsd:documentation>        </xsd:annotation>      </xsd:element>      <xsd:element name="ClearedIncident" type="xsd:boolean" minOccurs="0">        <xsd:annotation>          <xsd:documentation/>        </xsd:annotation>      </xsd:element>      <xsd:element minOccurs="0" name="IncidentPriority" type="xsd:integer" />      <xsd:element minOccurs="0" name="P0Summary" type="xsd:string" />    </xsd:sequence>  </xsd:complexType>  <xsd:complexType name="SourceStructure">    <xsd:annotation>      <xsd:documentation>Type for a source ie provider of information.</xsd:documentation>    </xsd:annotation>    <xsd:sequence>      <xsd:element name="TwitterHashtag" type="xsd:string" minOccurs="0">        <xsd:annotation>          <xsd:documentation>Twitter hash tag for the source.</xsd:documentation>        </xsd:annotation>      </xsd:element>    </xsd:sequence>  </xsd:complexType>  <xsd:complexType name="AffectsStructure">    <xsd:annotation>      <xsd:documentation>Type for Location model for scope of incident or effect.</xsd:documentation>    </xsd:annotation>    <xsd:sequence>      <xsd:element name="Operators" minOccurs="0">        <xsd:annotation>          <xsd:documentation>Operators affected by incident.</xsd:documentation>        </xsd:annotation>        <xsd:complexType>          <xsd:sequence>            <xsd:element name="AffectedOperator" type="AffectedOperatorStructure" maxOccurs="unbounded">              <xsd:annotation>                <xsd:documentation>Operators of services affected by incident.</xsd:documentation>              </xsd:annotation>            </xsd:element>          </xsd:sequence>        </xsd:complexType>      </xsd:element>      <xsd:element name="RoutesAffected" type="xsd:string" minOccurs="0"/>    </xsd:sequence>  </xsd:complexType>  <xsd:complexType name="InfoLinkStructure">    <xsd:annotation>      <xsd:documentation>Type for a general hyperlink.</xsd:documentation>    </xsd:annotation>    <xsd:sequence>      <xsd:element name="Uri" type="xsd:anyURI">        <xsd:annotation>          <xsd:documentation>URI for link.</xsd:documentation>        </xsd:annotation>      </xsd:element>      <xsd:element name="Label" type="com:PopulatedStringType" minOccurs="0">        <xsd:annotation>          <xsd:documentation>Label for Link</xsd:documentation>        </xsd:annotation>      </xsd:element>    </xsd:sequence>  </xsd:complexType>  <!--Imported from Xtis common -->  <xsd:complexType name="AffectedOperatorStructure">    <xsd:annotation>      <xsd:documentation>Type for Annotated reference to affected  Operator.</xsd:documentation>    </xsd:annotation>    <xsd:sequence>      <xsd:element name="OperatorRef" type="com:AtocCodeType">        <xsd:annotation>          <xsd:documentation>Identifier of Operator.</xsd:documentation>        </xsd:annotation>      </xsd:element>      <xsd:element name="OperatorName" type="com:PopulatedStringType" minOccurs="0">        <xsd:annotation>          <xsd:documentation>Public Name of Operator. Can be derived from OperatorRef.</xsd:documentation>        </xsd:annotation>      </xsd:element>    </xsd:sequence>  </xsd:complexType>  <!-- ======================================================================= -->  <!-- Simple types -->  <!--Imported from Xtis common -->  <xsd:simpleType name="IncidentIdentifierType">    <xsd:annotation>      <xsd:documentation>Type for a referenceUnique identifier of participant.</xsd:documentation>    </xsd:annotation>    <xsd:restriction base="xsd:NMTOKEN">      <xsd:pattern value="[A-Za-z0-9]{32}"/>    </xsd:restriction>  </xsd:simpleType>  <!-- ======================================================================= -->  <!-- Enumerations --></xsd:schema>